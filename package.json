{
  "name": "rasterex-viewer",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --watch",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "eslint": "NODE_OPTIONS='--max-old-space-size=8192' npx eslint",
    "eslint:check": "NODE_OPTIONS='--max-old-space-size=8192' npx eslint src/app --max-warnings 0",
    "eslint:fix": "NODE_OPTIONS='--max-old-space-size=8192' npx eslint src/app --fix",
    "format": "prettier --write src/**/*.{ts,html,scss,json}",
    "lint": "npm run eslint:check && npm run format"
  },
  "lint-staged": {
    "src/**/*.{ts,html}": [
      "ng-lint-staged lint --max-warnings 10 --project rasterex-viewer --"
    ]
  },
  "private": true,
  "dependencies": {
    "@angular-slider/ngx-slider": "20.0.0",
    "@angular/animations": "^20.1.4",
    "@angular/cdk": "^20.1.4",
    "@angular/common": "^20.1.4",
    "@angular/compiler": "^20.1.4",
    "@angular/core": "^20.1.4",
    "@angular/forms": "^20.1.4",
    "@angular/platform-browser": "^20.1.4",
    "@angular/platform-browser-dynamic": "^20.1.4",
    "@angular/router": "^20.1.4",
    "@types/lodash": "^4.17.20",
    "angular2-draggable": "^16.0.0",
    "dayjs": "^1.11.9",
    "dompurify": "^3.1.5",
    "hex-rgb": "^5.0.0",
    "idb": "^8.0.0",
    "leader-line": "^1.0.7",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "ngx-color": "^9.0.0",
    "ngx-indexed-db": "^19.0.0",
    "ngx-toastr": "^18.0.0",
    "rgb-hex": "^4.0.1",
    "rxjs": "~7.8.0",
    "socket.io-client": "^4.8.1",
    "tslib": "2.8.1",
    "zone.js": "0.15.1"
  },
  "devDependencies": {
    "@angular-eslint/builder": "20.1.1",
    "@angular-eslint/eslint-plugin": "20.1.1",
    "@angular-eslint/eslint-plugin-template": "20.1.1",
    "@angular-eslint/schematics": "20.1.1",
    "@angular-eslint/template-parser": "20.1.1",
    "@angular/build": "^20.1.4",
    "@stylistic/eslint-plugin": "^5.2.2",
    "@typescript-eslint/eslint-plugin": "8.38.0",
    "@typescript-eslint/parser": "8.38.0",
    "@angular/cli": "~20.1.4",
    "@angular/compiler-cli": "^20.1.4",
    "@types/lodash-es": "^4.17.8",
    "eslint": "9.32.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-no-null": "1.0.2",
    "eslint-plugin-unused-imports": "^4.1.4",
    "lint-staged": "16.1.2",
    "prettier": "3.5.3",
    "ts-node": "10.9.2",
    "typescript": "~5.8.3"
  }
}
